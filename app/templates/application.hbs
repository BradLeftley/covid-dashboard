<div class="container">
    <div class="col-md-12">
        <h4 class="header">
            Daily Update
        </h4>
        <h5 class="subheader">{{moment-format @model.firstObject.date 'MMMM Do YYYY'}}</h5>
    </div>
    {{#each model as |country|}}
        <DataCard @country={{country.areaName}} @amount={{country.newCasesByPublishDate}} />
        <br>
    {{/each}}
</div>
{{outlet}}