<div class="container">
    <div class="col-md-12">
        <h4 class="header">
            Daily Update
        </h4>
        <h5 class="subheader">{{moment-format @model.firstObject.date 'MMMM Do YYYY'}}</h5>
    </div>
    {{#each model as |country|}}
    {{log country.today.newCasesByPublishDate}}
        <DataCard @country={{country.countryName}} @todaysAmount={{country.today.newCasesByPublishDate}} @yesterdaysAmount={{country.yesterday.newCasesByPublishDate}}  />
        <br>
    {{else}}
    <h5>Update not recieved yet</h5>
    {{/each}}
    {{log model}}
</div>
{{outlet}}